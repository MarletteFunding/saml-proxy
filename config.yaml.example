listen_interface: 127.0.0.1
listen_port: 9090

targets:
  - http://localhost:8080

rate_limit_avg_minute: 500
rate_limit_burst_second: 50

idp_metadata_url: https://some.idp/metadata/32093329
service_root_url: http://localhost:9090
cert: myservice.cert
key: myservice.key

cookie_max_age: 4h

# panic, fatal, error, warn, info, debug
# debug level will log cookie information! this can lead to session hijacking!
log_level: warn
